{
  "manifest_version": 2,
  "description": "Face the day and never forget what's on your calendar.",
  "name": "Today's the Day",
  "version": "0.1",
  "chrome_url_overrides" : {
    "newtab": "todayhome.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["todayhomestyle.css"],
      "js": ["jquery-3.1.1.min.js", "main.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "icons": {
	"128": "icon.png" }
}